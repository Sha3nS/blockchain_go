package main

import (
"bytes"
"encoding/gob"
)

func (b *Block) Serialize() []byte {
  var result bytes.Buffer
  encoder := gob.NewEncoder(&result)

  err := encoder.Encode(b) //FIXME
  return result.Bytes
}

